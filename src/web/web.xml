<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <servlet>
    <servlet-name>Phenote</servlet-name>
    <servlet-class>phenote.servlet.PhenoteServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
     <servlet-name>Phenote</servlet-name>
     <!-- url-pattern>/Phenote</url-pattern -->
     <url-pattern>/phenote/Phenote</url-pattern>
  </servlet-mapping>
</web-app>
