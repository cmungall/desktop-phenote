                          <void property="value">
                            <string>int maxMemory = new Long(SystemInfo.getPhysicalMemory() / 1024 / 1024).intValue();		
// the 'os.arch' property gives you the bits for the running JavaVM not the OS.
String arch = System.getProperty("os.arch");
boolean is64bit = (arch != null) &amp;&amp; (arch.contains("x86_64") || arch.contains("amd64"));
if(!is64bit) {
	int limit = 1860;
	if(Util.isWindows()) {
		// 32 bit windows JavaVM has lower limits.
		limit = 1024;
	}
	maxMemory = Math.min(maxMemory, limit);
}
return maxMemory;
</string>
