                <initScript>int val;
int maxMemory = (Integer) context.getVariable("phenote.maxMemorySize");
if (maxMemory &lt;= 1860) {
    val = maxMemory;
}
else {
    val = (int) Math.max(((Math.round(maxMemory * 0.6) / 128) *128), 1860);
}
// create a new model for the spinner
